                  <select id="role" onchange="searchEmployeeByRole(1,$(this).find('option:selected').val())">
                  </select>
                  
                  <label>所属部门：</label>
                  <select id="department" onchange="searchEmployeeByDepartment(1,$(this).find('option:selected').val())">
                  </select>
                  
                   
                  <label>级别：</label>
                  <select id="level" onchange="searchEmployeeByLevel(1,$(this).find('option:selected').val())" style="width: 120px;">
                      <option value="1">1级</option>
                      <option value="2">2级</option>
                      <option value="3">3级</option>
                      <option value="4">4级</option>
                      <option value="5">5级</option>
                  </select>
                  
                  <label>部门经理：</label>
                  <select id="depManager" onchange="searchEmployeeByDepManager(1,$(this).find('option:selected').val())">
                  
                  </select>
                  
                  
                  <label>年龄：</label>
                  <select id="empAge" onchange="searchEmployeeByEmpAgeInterval(1,$(this).find('option:selected').val())">
                       <option value="18,20">18-20岁</option>
                       <option value="20,25">20-25岁</option>
                       <option value="25,30">25-30岁</option>
                       <option value="30,35">30-35岁</option>
                       <option value="35,40">35-40岁</option>
                       <option value="40,45">40-45岁</option>
                       <option value="45,50">45-50岁</option>
                       <option value="50,55">50-55岁</option>
                       <option value="55,60">55-60岁</option>
                       <option value="60,100">60岁以上</option>
                  </select><br>
                  
                  <label>工龄：</label>
                  <select id="empWorkingYears" onchange="searchEmployeeByEmpWorkingYearsInterval(1,$(this).find('option:selected').val())">
                       <option value="0,1">1年以下</option>
                       <option value="1,2">1-2年</option>
                       <option value="3,5">3-5年</option>
                       <option value="5,10">5-10年</option>
                       <option value="10,20">10-20年</option>
                       <option value="20,50">20-50年</option>
                       <option value="50,100">50年以上</option>
                  </select>
                  
                  
                      
                  <label>月薪：</label>
                  <select id="monthPay" onchange="searchEmployeeByMonthPayInterval(1,$(this).find('option:selected').val())">
