    <script>
     $("#addEmployee").on('click',function(){
 		/* alert(1); */
 		
 		$(".zhezhao").show();
 		$(".add-op").show();
 		$("#btn_save_employee").click(function(){
 			var empName=$("#addEmpName").val();
 			var empSex=$(":radio:checked").val();
 			var empAge=$("#addEmpAge").val();
 			
 			
 			var empNo=$("#addEmpNo").val();
 			var empRemark=$("#addEmpRemark").val();
 			
 			var role=$("#addRole").find("option:selected").val();
 			var dep=$("#addDep").find("option:selected").val();
 			
 			//下拉框部门的改变引起部门经理的变化
 			$("#addDep").change(function(){
 				//根据depId获取dep
 				var $addDepManager=$("#addDepManager");
 				$addDepManager.text(dep.depManager);
 			});
 			
 			//向后台发送ajax请求保存数据到数据库中
 			sendAjaxRequestToSaveEmployee(role,dep,empName,empSex,empAge,empNo,empRemark);
 		});
 		/* alert(role.roleName); */
 	});
     
     </script>
