.zhezhao
{
  width: 100%;
  height: 855px;
  background:rgba(0,0,0,.4);
  position: absolute;
  left: 0;
  top:0;
  z-index: 3;
  display: none;
}
.close
{
  display: block;
  position: absolute;
  height:28px;
  width: 28px;
  
  background: url(../img/close.png);
  background-size: cover;
  right: -12px;
  top: -12px;
  cursor: pointer;
}
.kongxian-op
{
  margin:0 auto;
  margin-top:250px;
  width: 500px;
  height:300px;
  background:#fff;
  box-shadow: 0 5px 5px #777;
  position: relative;
  display: none;
  z-index: 5;
}
.kongxian-op span
{
  font-size: 16px;
  color:#3399CC;
}
.kongxian-i1,.kongxian-i2
{
  width: 50px;
  height:40px;
  margin:0 auto;
  /*line-height:50px;*/
