package com.hs.entity;

public class User {
    private Integer userid;

    private String empnumber;

    private String password;

    private Integer roleid;

    @Override
	public String toString() {
		return "User [userid=" + userid + ", empnumber=" + empnumber + ", password=" + password + ", roleid=" + roleid
				+ "]";
	}

	public User() {
		super();
		// TODO Auto-generated constructor stub
	}

	public User(Integer userid, String empnumber, String password, Integer roleid) {
		super();
		this.userid = userid;
		this.empnumber = empnumber;
		this.password = password;
		this.roleid = roleid;
	}

	public Integer getUserid() {
        return userid;
    }

    public void setUserid(Integer userid) {
        this.userid = userid;
    }

    public String getEmpnumber() {
        return empnumber;
    }

    public void setEmpnumber(String empnumber) {
        this.empnumber = empnumber == null ? null : empnumber.trim();
    }

    public String getPassword() {
        return password;
    }

    public void setPassword(String password) {
        this.password = password == null ? null : password.trim();
    }

    public Integer getRoleid() {
        return roleid;
    }

    public void setRoleid(Integer roleid) {
        this.roleid = roleid;
    }
}