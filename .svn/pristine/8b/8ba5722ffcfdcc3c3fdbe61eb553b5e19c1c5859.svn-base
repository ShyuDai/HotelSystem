        <div class="zhezhao">
            <div class="kongxian-op">
            	<form action="">
	              <div class="kongxian-tittle">
	                <span class="close"></span>
	                请选择入住或预定
	              </div>
	              <div class="kongxian-i1"><input type="radio" name="kongxian" value="2" checked="checked" /><span>入住</span></div>
	              <div class="kongxian-i2"><input type="radio" name="kongxian" value="1" /><span>预定</span></div>
	              <span id="btnNum1">保存</span>
              </form>
            </div>
            
            <div class="yuding-op">
            	<form action="">
	              <div class="yuding-tittle">
	                <span class="close"></span>
	                请选择入住或取消
	              </div>
	              <div class="yuding-i1"><input type="radio" name="yuding" value="2" checked="checked" /><span>入住</span></div>
	              <!-- <div class="yuding-i2"><input type="radio" name="yuding" value="1" /><span>预定</span></div> -->
	              <div class="yuding-i3"><input type="radio" name="yuding" value="0"  /><span>取消</span></div>
	              <button class="btnNum">保存</button>
	             </form>
            </div>
            <div class="ruzhu-op">
            	<form action="">
	              <div class="ruzhu-tittle">
	                <span class="close"></span>
	                是否退房
	              </div>
	              <div class="ruzhu-i1"><input type="radio" name="ruzhu" value="0" checked="checked" /><span>是</span></div>
	              <div class="ruzhu-i2"><input type="radio" name="ruzhu" value="2" /><span>否</span></div>
	              <button class="btnNum">保存</button>
	             </form>
            </div>
            
            <div class="kongxian-op-news">
            	<form action="CheckInwgServlet?flag=0" method="post">
	              <div class="kongxian-news-tittle">
	                <span class="close"></span>
	                请填写入住用户信息
	              </div>
	              <div class="news-i1"><span>姓名:</span><input type="text" name="customername"/></div>
	              <div class="news-i2"><span>电话:</span><input type="text" name="customerphone"/></div>
	              
	              <div class="news-i3"><span>身份证号:</span><input type="text" name="customeridcard"/></div>
	              <div class="news-i4">男：<input id="chSex" type="radio" name="customersex" value="1" checked="checked" />
	              										女：<input id="chSex" type="radio" name="customersex" value="0" /></div>
           			<input type="hidden" name="roomnumber" id="hideNum">
	              <button type="submit" class="btnNum2">保存</button>
	             </form>
            </div>
            
            <div class="kongxian-op-new">
            	<form action="OrderRoomServletwg" method="post">
	              <div class="kongxian-new-tittle">
	                <span class="close"></span>
	                请填写预定用户信息
	              </div>
	              <div class="new-i1"><span>姓名:</span><input type="text" name="customername"/></div>
	              <div class="new-i2"><span>电话:</span><input type="text" name="customerphone"/></div>
	              
	              <div class="new-i3"><span>身份证号:</span><input type="text" name="customeridcard"/></div>
	              <div class="new-i4">男：<input id="chSex" type="radio" name="customersex" value="1" checked="checked" />
	              										女：<input id="chSex" type="radio" name="customersex" value="0" /></div>
           			<input type="hidden" name="roomnumber" id="hideNum1">
	              <button type="submit" class="btnNum2">保存</button>
	             </form>
