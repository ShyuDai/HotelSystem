               <div>
                 <label>性别：</label>
                 <select id="sex" onchange="searchEmployeeBySex(1,$(this).find('option:selected').val())" style="width: 120px;">
                    <option value="男">男</option>
                    <option value="女">女</option>
                 </select>
                 
