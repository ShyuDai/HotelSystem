#table
{
	width: 1300px;
	height: 800px;
	display:inline-block;
	float:left;
}
#table div
{
	width: 100px;
	height:100px;
	border-radius: 10px;
	float: left;
	margin-left: 20px;
	margin-top: 20px;
	cursor:pointer;
	color:#333;
	background:#fffbf0;
	box-shadow: 0px 5px 8px #444;
}
#table div span
{
	display:block;
	background:green;
	width:100%;
	height:25px;
	border-top-left-radius:5px;
	border-top-right-radius:5px;
}
#table div b
{
	margin-left:23px;
}
